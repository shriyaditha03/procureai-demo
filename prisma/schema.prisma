generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model DemoRequest {
  id           String   @id @default(cuid())
  fullName     String
  organization String
  email        String
  phone        String?
  industry     String
  message      String
  createdAt    DateTime @default(now())
}
